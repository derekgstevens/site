</section>

<section id="quantself">
	<div class="day">
		<h1><%= "#{@day.month.name} #{@day.day_date.day.ordinalize}" %></h1>
		<p>Sleep: <%= @day.min_slept %> | Steps: <%= @day.step_total %></p>
		<p>Commits: <%= @day.commits %></p>
    

    <div class="bar"></div>
    <ul class="storyline">
    <% @day.prepare_storyline.each do |story| %>
      <li style="left: <%= ((story[:startTime] - @day.day_date.beginning_of_day) / 86400) * 100 %>%; width: <%= story[:width] %>%;">
        <time class="tmtime" datetime="<%= "#{story[:startTime]}" %>"><span><%= story[:startTime].strftime("%H:%M") %></span></time>
        <!-- <div class="tmicon <%= "#{story[:type]}" %>"></div> -->
        <div class="tmlabel <%= "#{story[:name]}" %>"><h2><%= story[:name] %></h2></div>
      </li>
    <% end %>
    </ul>
	</div>
</section>

